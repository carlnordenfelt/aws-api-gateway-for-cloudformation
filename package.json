{
  "name": "AWS-APIGateway-for-CloudFormation",
  "version": "1.1.6",
  "description": "AWS Api Gateway CloudFormation",
  "author": "carl@nordenfelt.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlnordenfelt/aws-api-gateway-for-cloudformation"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules *.zip coverage && npm install",
    "test": "npm run eslint && NODE_ENV=TEST npm run unittest && npm run checkcoverage",
    "unittest": "istanbul cover --include-all-sources true -x \"_scripts/**/*\" ./node_modules/mocha/bin/_mocha ./tests/unit -- --recursive",
    "checkcoverage": "istanbul check-coverage --statement 100 --branches 100 --functions 100 --lines 100",
    "eslint": "eslint --max-warnings 0 lib"
  },
  "dependencies": {
    "async": "1.5.0",
    "aws-sdk": "2.2.36",
    "deep-equal": "1.0.1",
    "lodash": "3.10.1"
  },
  "devDependencies": {
    "istanbul": "0.3.19",
    "mocha": "2.3.1",
    "mockery": "1.4.0",
    "should": "7.1.0",
    "sinon": "1.16.1",
    "chai": "3.5.0",
    "eslint": "1.10.1",
    "eslint-config-defaults": "7.1.1",
    "eslint-plugin-filenames": "0.2.0"
  }
}
